{
  "name": "tittle",
  "version": "0.0.0-semantically-released",
  "description": "tittle",
  "main": "dist/tittle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bikingmonk/tittle.git"
  },
  "author": "bikingmonk <bikingmonk@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "commitizen": "3.0.5",
    "commitlint": "7.3.2",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.12.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.1",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-merge": "4.2.1"
  },
  "scripts": {
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "lint": "eslint .",
    "commit": "git-cz",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack --env.mode development",
    "validate": "run-p lint build",
    "release": "semantic-release --ci false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn validate",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
